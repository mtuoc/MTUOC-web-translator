{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "ODTCleaner",
            "type": "debugpy",
            "request": "launch",
            "program": "MTUOC_cleanODT.py",
            "console": "integratedTerminal",
            "args": [
                "in.odt",
                "clean_in.xml"
            ]
        },
        {
            "name": "DOCXCleaner",
            "type": "debugpy",
            "request": "launch",
            "program": "MTUOC_cleanDOCX.py",
            "console": "integratedTerminal",
            "args": [
                "testing/in_formatted.docx",
                "testing/out.docx",
                "--unzip"
            ]
        },
        {
            "name": "Python: Streamlit",
            "type": "debugpy",
            "request": "launch",
            "module": "streamlit",
            "args": [
                "run",
                "MTUOC-web-translator-MOD.py",
                "--server.headless",
                "true"
            ]
        }
    ]
